@model IEnumerable<Entidades.recPerfiles_Result>

@{
    ViewBag.Title = "listarPerfiles_PA";
}

<h2>Perfiles</h2>

<p>
    @Html.ActionLink("nuevo", "agregarPerfiles_PA")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Perfil_Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.nombre_perfil)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Perfil_Id)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.nombre_perfil)
        </td>
        <td>
            @Html.ActionLink("Editar", "modificarPerfiles_PA", new { pId=item.Perfil_Id }) |
            @Html.ActionLink("Detalle", "detallePerfiles_PA", new { pId = item.Perfil_Id }) |
            @Html.ActionLink("Borrar", "eliminarPerfiles_PA", new { pId = item.Perfil_Id })
        </td>
    </tr>
}

</table>
