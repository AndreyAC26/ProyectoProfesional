@model IEnumerable<MVCMuncheese.Models.modeloProveedores>

    ViewBag.Title = "listarProveedores_PA";
}

<h2>Proveedores</h2>

<p>
    @Html.ActionLink("Nuevo", "agregarProveedores_PA")
</p>
<table class="table">
    <tr>
        @*<th>
            @Html.DisplayNameFor(model => model.Id_Proveedor)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.Nombre)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido_1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Apellido_2)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Telefono)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Producto)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @*<td>
            @Html.DisplayFor(modelItem => item.Id_Proveedor)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.Nombre)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellido_1)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Apellido_2)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Telefono)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Producto)
        </td>
        <td>
            @Html.ActionLink("Editar", "modificarProveedores_PA", new { pId=item.Id_Proveedor }) |
            @Html.ActionLink("Detalle", "detalleProveedores_PA", new { pId = item.Id_Proveedor }) @*|
            @Html.ActionLink("Borrar", "eliminarProveedores_PA", new { pId = item.Id_Proveedor })
        </td>*@
    </tr>
}

</table>
